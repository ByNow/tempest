<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.bynow.tempest.web.modular.system.mapper.DictMapper">

    <resultMap type="cn.bynow.tempest.web.modular.system.model.Dict" id="dictMap">
        <result property="id" column="id"/>
        <result property="num" column="num"/>
        <result property="pid" column="pid"/>
        <result property="name" column="name"/>
        <result property="tips" column="tips"/>
    </resultMap>

    <select id="selectByPrimaryKey" resultType="cn.bynow.tempest.web.modular.system.model.Dict">
		SELECT * FROM sys_dict WHERE id = #{id}
	</select>

    <select id="selectByParams" resultMap="dictMap">
        SELECT * FROM sys_dict
        <where>
            <if test="id != null">
                <![CDATA[AND id = #{id} ]]>,
            </if>
            <if test="num != null">
                <![CDATA[AND num = #{num} ]]>,
            </if>
            <if test="pid != null">
                <![CDATA[AND pid = #{pid} ]]>,
            </if>
            <if test="name != null">
                <![CDATA[AND name = #{name} ]]>,
            </if>
            <if test="tips != null">
                <![CDATA[AND tips = #{tips} ]]>,
            </if>
        </where>
        ORDER BY id DESC
    </select>

    <insert id="insertSelective" parameterType="cn.bynow.tempest.web.modular.system.model.Dict" useGeneratedKeys="true"
            keyProperty="id">
        INSERT INTO sys_dict
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">
                id,
            </if>
            <if test="num != null">
                num,
            </if>
            <if test="pid != null">
                pid,
            </if>
            <if test="name != null">
                name,
            </if>
            <if test="tips != null">
                tips,
            </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id != null">
                #{id},
            </if>
            <if test="num != null">
                #{num},
            </if>
            <if test="pid != null">
                #{pid},
            </if>
            <if test="name != null">
                #{name},
            </if>
            <if test="tips != null">
                #{tips},
            </if>
        </trim>
    </insert>

    <update id="updateByPrimaryKeySelective" parameterType="cn.bynow.tempest.web.modular.system.model.Dict">
        UPDATE
        sys_dict
        <set>
            <if test="id != null">
                <![CDATA[ id = #{id} ]]>,
            </if>
            <if test="num != null">
                <![CDATA[ num = #{num} ]]>,
            </if>
            <if test="pid != null">
                <![CDATA[ pid = #{pid} ]]>,
            </if>
            <if test="name != null">
                <![CDATA[ name = #{name} ]]>,
            </if>
            <if test="tips != null">
                <![CDATA[ tips = #{tips} ]]>,
            </if>
        </set>
        WHERE id = #{id}
    </update>

    <delete id="deleteByPrimaryKey">
      DELETE FROM sys_dict  WHERE id = #{id}
    </delete>

</mapper>